# 数据库配置
DATABASE_URL=mysql://root:password@localhost:3306/quantconsole

# JWT 密钥
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production

# 服务器配置
HOST=127.0.0.1
PORT=8080

# 日志级别
